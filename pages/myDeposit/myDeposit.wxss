/* pages/myDeposit/myDeposit.wxss */

.myDeposit {
  background: rgba(242, 242, 242, 1);
  height: 100vh;
  padding: 20rpx 0rpx 0rpx;
  font-family: MicrosoftYaHei;
  box-sizing:border-box;
}

.md {
  background: #fff;
  padding:5rpx 0rpx 0rpx 17rpx;
}
.md>view{
  height:80rpx;
  line-height:80rpx;
  overflow:hidden;
  padding-right:21rpx;
  border-bottom:1rpx solid rgba(227,227,227,1);
}
.md>view text {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}
.md>view text:nth-child(2){
  float:right;
}
.md>view .iconfont{
  font-size:28rpx;
 color:rgba(185,185,185,1)
}