/* pages/withFail/withFail.wxss */
/* pages/withSuccess/withSuccess.wxss */

.withFail {
  text-align: center;
  box-sizing: border-box;
}

.ws-top {
  margin-top: 59rpx;
}

.ws-top text {
  display: block;
}

.ws-top>text:nth-child(1) {
  font-size: 100rpx;
  color: rgba(95, 163, 239, 1);
  height: 100rpx;
}

.ws-top>text:nth-child(2) {
  margin-top: 37rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 30rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  margin-bottom: 22rpx;
}

.ws-top>text:nth-child(3) {
  font-size: 26rpx;
  color: rgba(153, 153, 153, 1);
}

.ws-btn {
  margin-top: 186rpx;
}

.ws-btn button {
  width: 710rpx;
  height: 80rpx;
  background: rgba(95, 163, 239, 1);
  border-radius: 40rpx;
  line-height:80rpx;
  font-size:34rpx;
  font-weight:bold;
  color:rgba(254,254,254,1);
}
