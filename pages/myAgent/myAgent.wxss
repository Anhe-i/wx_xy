/* pages/myAgent/myAgent.wxss */
.myAgent{
  padding-left:20rpx;
  box-sizing:border-box;
}
.myAgent>view {
  display: flex;
  justify-items: center;
  font-family: MicrosoftYaHei;
  font-weight: 400;
}

.myAgent>view>view {
  width: 50%;
  height:83rpx;
  border-bottom:1rpx solid rgba(227,227,227,1);
  line-height:83rpx;
}

.myAgent>view>view>text:nth-child(1) {
  font-size: 35rpx;
  color: #5fa3ef;
  margin-right:12rpx;
}

.myAgent>view>view>text:nth-child(2) {
  font-size: 28rpx;
  color: rgba(102, 102, 102, 1);
}
